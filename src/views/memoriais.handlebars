<div class="tituloMemoriais">
    <p>Memoriais em nosso site</p> 
</div>

<div class="conteudo">
    {{#each memorial}}
        <div class="memoriais">
            <img class="imagem" src="{{imagem}}" alt="imagem" width="200" height="120">
            <p>{{nome}}</p>
            <p>{{formatDate data_nascimento}} - {{formatDate data_falecimento}}</p>
            <p>{{local_nascimento}} - {{local_falecimento}}</p>
        </div>
    {{/each}}
</div>